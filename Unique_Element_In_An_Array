#include<iostream>
using namespace std;
int main()
{
    int n=7;
    int arr[]={1,2,3,2,1,3,5};
    cout<<"original array is"<<endl;
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    cout<<"unique elements in the array is:-"<<endl;
    for(int i=0;i<n;i++){
        int count=0;
        for(int j=0;j<n;j++){
            if(arr[i]==arr[j])
            count++;
            
        }
        if(count==1){
            cout<<arr[i]<<endl;
        }
    }
}

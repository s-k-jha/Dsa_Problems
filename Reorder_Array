#include <iostream>
using namespace std;

int main() {
    int arr[]={1,2,3,4};
    int n=sizeof(arr)/sizeof(arr[0]);
    int st=0;
    int end=n-1;
    for(int i=0;i<n;i++)
    {
        while(st<end)
        {
            
             cout<<arr[st]<<arr[end];
             st++;
             end--;
             if(st==end){
                cout<<arr[st];
            }
        }
        
    }
    return 0;
}
